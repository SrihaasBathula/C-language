#include <stdio.h>
void main()
{
    for (int i=2;i<1000;i++) 
    {
        int count = 0; 
        for (int j=2;j<=i/2;j++) 
        {
            if (i%j==0)
            {
                count++;
            }
        }
        
        if (count==0) {
            printf("%d\n",i);
        }
    }

}
